    object PrefixSuffix: TTabSheet
      Caption = 'Callsign Suffix/Prefix'
      ClientHeight = 514
      ClientWidth = 759
      TabVisible = False
      object Label1: TLabel
        Left = 4
        Height = 343
        Top = 5
        Width = 724
        Caption = 'Using a suffix or prefix with JT65 should only be selected when you must do so for rule'#13#10'compliance.'#13#10#13#10'You may select a suffix OR a prefix but not both.'#13#10#13#10'A suffix or prefix value must be one of the pre-defined values defined by the JT65 protocol.  Use'#13#10'of suffix or prefix values not defined by the JT65 protocol is not allowed.'#13#10#13#10'Using a suffix or prefix changes the message format of the JT65 protocol to accomodate the'#13#10'suffix or prefix value leading to shortened structured messages and loss of ability to transmit'#13#10'your grid value via structured messages.'#13#10#13#10'Unless you are legally required to sign with a suffix or prefix I strongly advise you not use either'#13#10'and send your suffixed or prefixed callsign in free text as a closing message for the QSO.  You'#13#10'may also select to send a CW ID with a suffixed/prefixed callsign where the suffix/prefix is not'#13#10'restricted to the JT65 protocol values IF it is not too long to be sent at the end of TX period.'#13#10#13#10'If you do not need suffix or prefix callsign support click the continue button.'
        Font.Height = -16
        Font.Name = 'Arial'
        Font.Style = [fsBold]
        ParentColor = False
        ParentFont = False
      end
